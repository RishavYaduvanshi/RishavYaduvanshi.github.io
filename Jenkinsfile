pipeline {
  agent none
  stages {
    stage('Build') {
      agent any
      steps {
        echo 'Starting Build'
        echo 'Building Project'
        echo 'Build Success'
      }
    }
    
    stage('Test') {
      agent any
      steps {
        echo 'Starting Testing'
        echo 'Testing Project'
        echo 'Testing Complete'
      }
    }
    
    
  }
}
    
  
